{
  "name": "moveit-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "initialize-db": "yarn create-db-collections && yarn create-db-collections",
    "create-db-collections": "fauna eval moveit --file=./scripts/db-queries/db-collections.fql",
    "create-db-indexes": "fauna eval moveit --file=./scripts/db-queries/db-indexes.fql"
  },
  "peerOptionalDependencies": {
    "mongodb": "^3.6.4"
  },
  "dependencies": {
    "@emotion/stylis": "^0.8.5",
    "@next/bundle-analyzer": "^10.0.8",
    "@types/js-cookie": "^2.2.6",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.18.0",
    "faunadb": "^4.8.0",
    "framer-motion": "^4.0.0",
    "js-cookie": "^2.2.1",
    "mongodb": "^3.6.4",
    "next": "^13.4.4",
    "next-auth": "^4.22.1",
    "nprogress": "^0.2.0",
    "polished": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.0-rc.3",
    "swr": "^0.5.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/mongodb": "^3.6.8",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^14.14.31",
    "@types/nprogress": "^0.2.0",
    "@types/react-dom": "^18.2.4",
    "@types/styled-components": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "@vercel/node": "^1.9.0",
    "fauna-shell": "^0.15.0",
    "typescript": "^5.1.3"
  }
